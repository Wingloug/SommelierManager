- if (extra.user_object_id)
	!= "<script type='text/javascript' id='extra'>"
	!= "var extra = {};"
	!= "extra.board_id = " + extra.board_id + ";"
	!= "extra.task = '" + extra.task + "';"
	!= "extra.task_description = '" + (extra.task_description ? extra.task_description : "") + "';"
	!= "extra.progress = " + extra.progress + ";"
	!= "</script>"

//- Si existe un panel para ver
- if (extra.board_id)
	.panel.panel-default
		.panel-heading
			h3.panel-title To Do
		.panel-body
			.row
				.col-md-4
					.panel.panel-info
						.panel-heading To Do
						.panel-body.board-container
							- for (var i = 0; i < todo.length; i++)
								div(draggable="true", id=todo[i].id, class=(todo[i].label ? "alert-" + todo[i].label : "alert-default")).board-item.alert
									div(style="margin-top: -18px; font-size: 1.3em").labels-container.pull-right
										a(class="item-label default " + (todo[i].label == "default" ? "selected" : ""))
											span(class="glyphicon glyphicon-bookmark")
										a(class="item-label success " + (todo[i].label == "success" ? "selected" : ""))
											span(class="glyphicon glyphicon-bookmark")
										a(class="item-label info " + (todo[i].label == "info" ? "selected" : ""))
											span(class="glyphicon glyphicon-bookmark")
										a(class="item-label warning " + (todo[i].label == "warning" ? "selected" : ""))
											span(class="glyphicon glyphicon-bookmark")
										a(class="item-label danger " + (todo[i].label == "danger" ? "selected" : ""))
											span(class="glyphicon glyphicon-bookmark")
										.clearfix
									p.item-content=todo[i].description
									p.actions.pull-right
										button(type="submit", data-target=todo[i].id, class="btn btn-xs btn-primary edit-item", data-toggle="tooltip", title="Editar")
											span.glyphicon.glyphicon-pencil
										=" "
										button(type="button", data-target=todo[i].id, class="btn btn-xs btn-default del-item", data-toggle="tooltip", title="Eliminar")
											span.glyphicon.glyphicon-trash
									.clearfix
						.panel-footer
							a.add-item.btn.btn-default.btn-sm.pull-right
								span.glyphicon.glyphicon-plus
								|  Agregar item
							.clearfix
				.col-md-4
					.panel.panel-info
